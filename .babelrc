{
  "presets": [
    ["es2015", {"modules": false}],
    "react"
  ],
  "plugins": [
    "transform-runtime",
    "transform-class-properties",
    "transform-decorators-legacy",
    "lodash",
    "react-hot-loader/babel"
  ],
  "env": {
    "test": {
      "presets": ["es2015", "react"],
      "plugins": ["transform-es2015-modules-commonjs", "transform-class-properties"]
    }
  }
}